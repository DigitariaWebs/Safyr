{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "start": {
      "persistent": true,
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {},
    "test": {
      "dependsOn": ["^build"]
    },
    "android": {
      "persistent": true,
      "cache": false
    },
    "ios": {
      "persistent": true,
      "cache": false
    }
  }
}
